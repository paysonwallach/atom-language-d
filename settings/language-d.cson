".source.d":
  editor:
    increaseIndentPattern: '''
      (?x)
            ^ .* \\{ [^}"']* $ |
            ^ \\s* (public|private|protected): \\s* $ |
            ^ \\s* \\{ \\} $

    '''
    decreaseIndentPattern: '''
      (?x)
            ^ (.*\\*/)? \\s* \\} ([^}{"']* \\{)?[;\\s]* (//.*|/\\*.*\\*/\\s*)? $ |
            ^ \\s* (public|private|protected): \\s* $

    '''
    foldEndPattern: "(?<!\\*)\\*\\*/|^\\s*\\}"
    commentStart: "// "
    completions: [
